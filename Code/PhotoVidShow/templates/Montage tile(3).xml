      <PanZoom InitialDelay="0.12" PanZoomOnAll="True">
        <StartArea X="0" Y="0" Width="1" Height="1" />
        <EndArea X="0.01818182" Y="0.01969058" Width="0.9596838" Height="0.9592124" />
