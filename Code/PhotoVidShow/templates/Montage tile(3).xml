          <Image Filename="image2" />
        </Decoration>
        <Decoration Type="ImageDecoration" Aspect="2" OriTemImageNum="3" StartOffsetTime="0.4" InEffect="MoveLeftMore">
          <AlphaMap Filename="outlinemasks\smooth edge.jpg" />
          <CoverageArea X="0" Y="0" Width="0.6629" Height="1" />
          <Image Filename="image3" />
        </Decoration>
      </Decoration>
