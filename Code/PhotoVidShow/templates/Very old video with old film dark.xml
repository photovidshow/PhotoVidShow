        </Decoration>
        <Decoration Type="VideoDecoration" VideoName="filters\old film(mod1x).pvsv" Volume="0" GlobalPlayer="True" FlipY="True" RenderMethod="4">
          <CoverageArea X="0" Y="0" Width="1" Height="1" />
        </Decoration>
        <Decoration Type="SepiaTransformDecoration">
          <CoverageArea X="0" Y="0" Width="1" Height="1" />
        </Decoration>
        <Decoration Type="SepiaTransformDecoration">
          <CoverageArea X="0" Y="0" Width="1" Height="1" />
        </Decoration>
        <Decoration Type="BlurFilterDecoration" BlurFilterType="1">
          <CoverageArea X="0" Y="0" Width="1" Height="1" />
        </Decoration>
      </Decoration>
      <PanZoom PanZoomOnAll="True" ReGenerateOnImageChange="True">
        <StartArea X="0.125" Y="0.125" Width="0.75" Height="0.75" />
        <EndArea X="0.21005" Y="0.21005" Width="0.5799" Height="0.5799" />
      </PanZoom>
    </Slide>
  </Slideshow>
  <DecorationEffectsDatabase />
