<Template Version="1.0.0" Name="Intro sequence zoom out fast" Description="Intro sequence with one line of text rapidly zooming out&#xD;&#xA;">
  <Slideshow>
    <Slide Type="BlankStillPictureSlide" PanZoom="False" DisplayTimeLength="7" UsesDefaultTransitionEffect="True" ThumbnailPreviewTime="5" Color="Color [Black]">
      <TransitionEffect Type="SimpleAlphaBlendTransitionEffect" Length="1.5" Index="1" />
      <Decoration Type="TextDecoration" InEffect="DelayMoveIn" Text="My slideshow presentation" HintEditTime="5">
        <CoverageArea X="0.21" Y="0.44" Width="0.58" Height="0.1201373" />
        <TextStyle Name="Vanilla" FontName="Segoe UI" FontSize="44.06217" OutLine="True" OutLineColour="Color [White]" Alignment="Center" OutlineLength="4" OutlineAlpha="14" />
      </Decoration>
      <PanZoom>
        <StartArea X="0" Y="0" Width="1" Height="1" />
        <EndArea X="0" Y="0" Width="1" Height="1" />
        <Equation Type="Linear" />
      </PanZoom>
    </Slide>
  </Slideshow>
  <DecorationEffectsDatabase>
    <DecorationEffect Name="DelayMoveIn" Length="1" MotionBlur="30" HasCharDelay="True" CharTimeOffset="0.075" CharOrder="0">
      <TransitionEffect Type="SimpleAlphaBlendTransitionEffect" Length="1.5" EndDelay="0.5" />
      <Movement Type="StraightLineMovement" InitialDelay="0" EndDelay="0">
        <Start X="0" Y="0" Z="-1" />
        <End X="0" Y="0" Z="1" />
        <Equation Type="NonLinear" />
      </Movement>
    </DecorationEffect>
  </DecorationEffectsDatabase>
</Template>