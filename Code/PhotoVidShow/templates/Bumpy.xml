<Template Version="1.0.0" Name="Bumpy" Description="Gives a slide a bumpy texture look to it.&#xD;&#xA;">
  <Slideshow>
    <Slide Type="BlankStillPictureSlide" PanZoom="True" DisplayTimeLength="8" UsesDefaultTransitionEffect="True" ReCalcSlideLengthOnImage1Change="True" Color="Color [Black]">
      <TransitionEffect Type="SimpleAlphaBlendTransitionEffect" Length="1.5" Index="1" />
      <Decoration Type="GroupedDecoration" MotionBlur="False" RenderMethod="7" MoveAsSlidePanZoom="True">
        <CoverageArea X="0" Y="0" Width="1" Height="1" />
        <Decoration Type="ImageDecoration" Aspect="3" OriTemImageNum="1">
          <CoverageArea X="0" Y="0" Width="1" Height="1" />
          <Image Filename="image1" />
        </Decoration>
        <Decoration Type="ImageDecoration" RenderMethod="5">
          <CoverageArea X="0" Y="0" Width="1" Height="1" />
          <Image Filename="filters\bumpy(mod2x).pvsi" />
        </Decoration>
      </Decoration>
      <PanZoom PanZoomOnAll="True" ReGenerateOnImageChange="True">
        <StartArea X="0.125" Y="0" Width="0.75" Height="0.75" />
        <EndArea X="0.125" Y="0.25" Width="0.75" Height="0.75" />
      </PanZoom>
    </Slide>
  </Slideshow>
  <DecorationEffectsDatabase />
</Template>