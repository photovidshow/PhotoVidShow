          <CoverageArea X="0" Y="0" Width="1" Height="1" />
          <Image Filename="image1" />
        </Decoration>
        <Decoration Type="ImageDecoration" Aspect="2" InEffect="Present2BottomDown" Brightness="1.92,1.97,2" Contrast="1,1,0.99">
          <CoverageArea X="0" Y="0" Width="1" Height="1.598948" />
          <Image Filename="templates\media\black.jpg" />
        </Decoration>
        <Decoration Type="ImageDecoration" DegreesRot="101" InEffect="Present2Bottomleft" Brightness="1.91,1.97,2">
          <AlphaMap Filename="outlinemasks\gradient edge.jpg" />
          <CoverageArea X="-0.5438735" Y="0.05344585" Width="1.039526" Height="0.7187061" />
          <Image Filename="templates\media\black.jpg" />
        </Decoration>
        <Decoration Type="ImageDecoration" DegreesRot="101" InEffect="Present2TopRightBadge" Brightness="1.91,1.97,2">
          <AlphaMap Filename="outlinemasks\gradient edge.jpg" />
          <CoverageArea X="0.3557312" Y="0.1068917" Width="1.089328" Height="0.2053446" />
          <Image Filename="templates\media\black.jpg" />
        </Decoration>
        <Decoration Type="ImageDecoration" DegreesRot="101" InEffect="Present2BackTopRight" Brightness="1.15,1.18,1.24">
          <AlphaMap Filename="outlinemasks\gradient edge.jpg" />
          <CoverageArea X="0.3581028" Y="0.2348804" Width="1.07668" Height="0.2236287" />
          <Image Filename="templates\media\black.jpg" />
        </Decoration>
        <Decoration Type="TextDecoration" DegreesRot="-79" StartOffsetTime="1.3" InEffect="Present2ScrollTextIn" Text="Text1 text1 text1 text1" TemplateEditable="1">
          <CoverageArea X="0.601581" Y="0.4542897" Width="0.487747" Height="0.1209564" />
          <TextStyle Name="Vanilla" FontName="Segoe UI" FontSize="51.00071" OutLine="True" OutLineColour="Color [White]" Alignment="Center" OutlineLength="4" OutlineAlpha="87" />
        </Decoration>
        <Decoration Type="ImageDecoration" DegreesRot="101" InEffect="Present2BlueTopRight" Brightness="1.04,1.28,1.49">
          <AlphaMap Filename="outlinemasks\smooth edge.jpg" />
          <CoverageArea X="0.4766798" Y="0.3544304" Width="1.016601" Height="0.3305204" />
          <Image Filename="templates\media\black.jpg" />
        </Decoration>
      </Decoration>
      <Decoration Type="ImageDecoration" Border="True">
        <CoverageArea X="0" Y="0" Width="1" Height="1" />
        <Image Filename="borders\vignette1.pvsi" />
      </Decoration>
      <PanZoom StartRotation="4.304692" EndRotation="0.3426961" PanZoomOnAll="True">
        <StartArea X="0.07826087" Y="0.06610408" Width="0.8695652" Height="0.8691983" />
        <EndArea X="0.06324111" Y="0.0604782" Width="0.8798419" Height="0.8790436" />
        <Equation Type="Linear" />
      </PanZoom>
    </Slide>
  </Slideshow>
